{
  "expo": {
    "name": "money",
    "slug": "money",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Ícone.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Ícone.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.slpjg007.money"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Ícone.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Ícone.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "11a1151f-9ae6-41bd-ad87-0bd5c32e3b20"
      }
    },
    "owner": "slpjg007"
  }
}
